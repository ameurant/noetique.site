    </type>

    <type type_id="Collection"/>
    <type type_id="Image"/>
    <type type_id="Folder"/>


    <type type_id="Collection">
      <bound-workflow workflow_id="one_state_workflow"/>
    </type>
    <type type_id="Image">
      <bound-workflow workflow_id="one_state_workflow"/>
    </type>
    <type type_id="Folder">
      <bound-workflow workflow_id="one_state_workflow"/>
    </type>


  </bindings>

</object>
